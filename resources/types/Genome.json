{
	"global-object-type": "Genome",
	"storage-type": "WS",
	"storage-object-type": "KBaseGenomes.Genome",
	"indexing-rules": [
		{
			"path": "id",
			"keyword-type": "string"
		},
		{
			"path": "domain",
			"keyword-type": "string"
		},
		{
			"path": "taxonomy",
			"full-text": true
		},
		{
			"path": "scientific_name",
			"full-text": true
		},
		{
			"path": "features/{size}",
			"keyword-type": "integer",
			"key-name": "features"
		},
		{
			"path": "assembly_ref",
			"transform": "guid",
			"target-object-type": "Assembly",
			"optional-default-value": [],
			"keyword-type": "string",
			"key-name": "assembly_guid",
			"ui-hidden": true
		},
		{
			"derived-key": true,
			"transform": "lookup.key.contigs",
			"source-key": "assembly_guid",
			"keyword-type": "integer",
			"key-name": "contigs"
		}
	]
}